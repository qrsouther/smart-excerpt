{
  "manifest_version": 3,
  "name": "MultiExcerpt Include Scanner",
  "version": "1.0.0",
  "description": "Scan for MultiExcerpt Includes in Confluence and export to CSV",
  "permissions": [
    "activeTab",
    "scripting",
    "clipboardWrite",
    "clipboardRead"
  ],
  "host_permissions": [
    "https://*.atlassian.net/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
